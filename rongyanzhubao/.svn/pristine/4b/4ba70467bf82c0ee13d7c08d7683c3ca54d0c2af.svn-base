package com.cn.jm._dao.room;

public enum RequerstEnum {
	/**
	 * 请求开播(或请求关联)
	 */
	REQUEST(0),
	/**
	 * 同意
	 */
	AGREE(1),
	/**
	 * 不同意
	 */
	DISAGREE(2),
	/**
	 * 已开播
	 */
	IS_SHOW(1),
	/**
	 * 未开播
	 */
	NOR_SHOW(0);
	
	private final int value;

	private RequerstEnum(int value) {
		this.value = value;
	}

	public int getValue() {
		return value;
	}

}
