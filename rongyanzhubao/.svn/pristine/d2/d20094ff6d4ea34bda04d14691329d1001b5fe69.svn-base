var sessionId;
var project;
layui.define(['table', 'form'], function(exports){
  var $ = layui.$
  ,table = layui.table
  ,form = layui.form
  ,element = layui.element;
  
  var ctx = $("#ctx").val();
  $("#anchor").show();
  $("#manage").show();
  form.on("radio(type)",function(data){
	  if(data.value==2){//商家
		  $("#manage").show();
		  $("#anchor").show();
	  }else{
		  $("#anchor").hide();
		  $("#manage").hide();
	  }
  })

  exports('toBind', {})
});













