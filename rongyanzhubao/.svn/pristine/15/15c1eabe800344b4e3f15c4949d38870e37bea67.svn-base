* {
	margin: 0;
	padding: 0
}

body {
	background-color: #f2f2f2
}

#app {
	background-color: #fff;
	width: 70%;
	min-width: 1080px;
	margin: 200px auto 0
}

#app 
.zent-form {
	display: flex;
	padding: 10px 10px
}

#app .zent-form .left {
	width: 100px;
	font-size: 12px;
	text-align: right
}

#app .zent-form .right {
	flex: 1;
	margin-left: 10px;
	padding: 10px;
	border: 1px solid #e5e5e5;
	background-color: #fff
}

#app .zent-form .right .rc-sku-group .mb {
	margin-bottom: 10px
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper {
	display: inline-block;
	margin-right: 10px
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper .zent-select
	{
	position: relative;
	height: 30px;
	width: 150px
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper .zent-select:hover span
	{
	display: inline-block
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper .zent-select span
	{
	display: none;
	position: absolute;
	top: -9px;
	right: -9px;
	color: #fff;
	text-align: center;
	cursor: pointer;
	width: 18px;
	height: 18px;
	line-height: 16px;
	font-size: 14px;
	background: rgba(153, 153, 153, 0.6);
	border-radius: 50%;
	z-index: 2
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper .zent-select input
	{
	position: absolute;
	width: 150px;
	height: 30px;
	border: 1px solid #bbb;
	padding: 0 5px;
	outline: none;
	box-sizing: border-box;
	border-radius: 3px
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper .zent-select select.typenum
	{
	width: 100%;
	position: absolute;
	top: 35px;
	z-index: 10
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper .zent-select select.typenum option
	{
	padding: 5px;
	cursor: pointer
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper .zent-select select.typenum option:hover
	{
	background-color: #1e91ff
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper 
.upload-img-wrap {
	width: 90px
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper .upload-img-wrap .arrow
	{
	width: 0;
	height: 0;
	margin: 0 auto;
	border-style: solid;
	border-color: transparent;
	border-left: 5px solid transparent;
	border-right: 5px solid transparent;
	border-bottom: 5px solid #000
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper .upload-img-wrap .arrow::after
	{
	display: block;
	width: 0;
	height: 0;
	border: 10px solid transparent;
	top: 0;
	margin-left: -10px;
	border-bottom-color: #fff;
	border-top-width: 0;
	content: ""
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper .upload-img-wrap .rc-upload
	{
	width: 90px;
	height: 90px;
	padding: 3px;
	border: 1px solid #dcdcdc;
	border-radius: 4px;
	box-sizing: border-box;
	display: flex;
	align-items: center;
	justify-content: center;
	position: relative;
	background-color: #fff
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper .upload-img-wrap .rc-upload:hover span.detele
	{
	display: inline-block
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper .upload-img-wrap .rc-upload span.detele
	{
	display: none;
	position: absolute;
	top: -9px;
	right: -9px;
	color: #fff;
	text-align: center;
	cursor: pointer;
	width: 18px;
	height: 18px;
	line-height: 16px;
	font-size: 14px;
	background: rgba(153, 153, 153, 0.6);
	border-radius: 50%;
	z-index: 10
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper .upload-img-wrap .rc-upload i.jia
	{
	font-weight: bold;
	font-size: 30px;
	font-style: normal;
	color: #ddd
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper .upload-img-wrap .rc-upload i.img
	{
	position: absolute;
	display: inline-block;
	width: 84px;
	height: 84px;
	margin: 0 auto;
	line-height: 84px;
	background-size: cover;
	background-position: center;
	background-repeat: no-repeat;
	background-color: #fff;
	z-index: 3
}

#app .zent-form .right .rc-sku-group .zent-popover-wrapper .upload-img-wrap .rc-upload input
	{
	position: absolute;
	width: 100%;
	height: 100%;
	opacity: 0;
	cursor: pointer;
	z-index: 9
}

#app .zent-form .right .rc-sku-group h3.group-title {
	position: relative;
	padding: 7px 10px;
	background-color: #f8f8f8;
	font-size: 12px;
	font-weight: 400;
	display: flex;
	align-items: center
}

#app .zent-form .right .rc-sku-group h3.group-title button {
	padding: 5px 10px;
	outline: none;
	border: 1px solid #ddd;
	background-color: #fff;
	border-radius: 3px
}

#app .zent-form .right .rc-sku-group h3.group-title button:hover {
	border-color: #38f;
	color: #38f;
	cursor: pointer
}

#app .zent-form .right .rc-sku-group h3.group-title button[disabled] {
	color: #bbb;
	background: #f8f8f8;
	border-color: #e5e5e5
}

#app .zent-form .right .rc-sku-group h3.group-title button[disabled]:hover
	{
	cursor: not-allowed
}

#app .zent-form .right .rc-sku-group h3.group-title span.group-title_label
	{
	width: 50px
}

#app .zent-form .right .rc-sku-group h3.group-title:hover span.group-remove
	{
	display: inline-block
}

#app .zent-form .right .rc-sku-group h3.group-title div.zent-checkbox-wrap
	{
	line-height: 36px;
	display: flex;
	align-items: center
}

#app .zent-form .right .rc-sku-group h3.group-title div.zent-checkbox-wrap input
	{
	margin-right: 5px
}

#app .zent-form .right .rc-sku-group h3.group-title span.group-remove {
	display: none;
	position: absolute;
	top: calc(50% - 9px);
	right: 10px;
	color: #fff;
	text-align: center;
	cursor: pointer;
	width: 18px;
	height: 18px;
	line-height: 16px;
	font-size: 14px;
	background: rgba(153, 153, 153, 0.6);
	border-radius: 50%
}

#app .zent-form .right .rc-sku-group .group-container {
	padding: 10px 10px 0;
	display: flex;
	font-size: 12px
}

#app .zent-form .right .rc-sku-group .group-container span.sku-list_label
	{
	width: 50px;
	font-size: 100%;
	margin-top: 6px
}

#app .zent-form .right .rc-sku-group .group-container div.sku-list-position
	{
	flex: 1;
	flex-wrap: wrap;
	display: flex
}

#app .zent-form .right .rc-sku-group .group-container div.sku-list-position .layui-input-inline
	{
	margin-right: 10px
}

#app .zent-form .right .rc-sku-group .group-container div.sku-list-position .layui-input-inline:hover span.group-remove
	{
	display: inline-block
}

#app .zent-form .right .rc-sku-group .group-container div.sku-list-position .layui-input-inline span.group-remove
	{
	display: none;
	position: absolute;
	top: -9px;
	right: -9px;
	color: #fff;
	text-align: center;
	cursor: pointer;
	width: 18px;
	height: 18px;
	line-height: 16px;
	font-size: 14px;
	background: rgba(153, 153, 153, 0.6);
	border-radius: 50%;
	z-index: 5
}

#app .zent-form .right .rc-sku-group .group-container div.sku-list-position .sku-list
	{
	display: inline-block;
	height: 30px;
	line-height: 30px;
	margin-bottom: 10px
}

#app .zent-form .right .rc-sku-group .group-container div.sku-list-position .sku-list a
	{
	color: #38f;
	text-decoration: none
}

#app .zent-stock {
	display: flex;
	padding: 10px 10px
}

#app .zent-stock .left {
	width: 100px;
	font-size: 12px;
	text-align: right
}

#app .zent-stock .right {
	flex: 1;
	margin-left: 10px;
	padding: 10px;
	border: 1px solid #e5e5e5;
	background-color: #fff
}

#app .zent-stock .right table {
	width: 100%;
	background-color: #fff;
	text-align: left;
	border-collapse: collapse;
	border-spacing: 0;
	font-size: 14px
}

#app .zent-stock .right table thead {
	border-bottom: 1px solid #e5e5e5
}

#app .zent-stock .right table thead tr th {
	color: #333;
	font-weight: normal;
	padding: 10px 8px
}

#app .zent-stock .right table tbody tr {
	border-bottom: 1px solid #e5e5e5
}

#app .zent-stock .right table tbody tr td {
	padding: 10px 8px;
	box-sizing: border-box
}

#app .zent-stock .right table tbody tr td input {
	height: 25px;
	width: 100px;
	border-radius: 3px;
	border: 1px solid #ddd;
	padding: 0 5px;
	outline: none
}

#app .zent-stock .right table tbody tr td input[type='number']::-webkit-outer-spin-button
	{
	-webkit-appearance: none !important;
	margin: 0
}

#app .zent-stock .right table tbody tr td input[type='number']::-webkit-inner-spin-button
	{
	-webkit-appearance: none !important;
	margin: 0
}

#app .zent-stock .right table tbody tr td input[type="number"] {
	-moz-appearance: textfield
}

#app .zent-stock .right table tbody tr td.active {
	border-bottom: 1px solid #e5e5e5
}

#app .zent-stock .right table tfoot tr td {
	padding: 10px 8px 0
}

#app .zent-stock .right table tfoot tr td span {
	color: #38f;
	cursor: pointer
}

#app .zent-stock .right table tfoot tr td in
put {
	width: 100px;
	height: 30px;
	padding: 0 5px;
	outline: none;
	border-radius: 3px;
	border: 1px solid #ddd
}